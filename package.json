  },
  "contributors": [
    "Andrew Plummer"
  ],
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "animation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anatolykopyl/vue-peel.git"
  },
  "browser": {
    "./sfc": "src/Peel.vue"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-peel.umd.js",
  "module": "./dist/vue-peel.js",
  "exports": {
    ".": {
